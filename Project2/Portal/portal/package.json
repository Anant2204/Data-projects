{
  "name": "mct",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "set HTTPS=true&&react-app-rewired start",
    "build": "npm run build:DEV1",
    "build:DEV1": "env-cmd -f .env.DEV1 react-app-rewired build",
    "build:UAT1": "env-cmd -f .env.UAT1 react-app-rewired build",
    "build:PROD": "env-cmd -f .env.PROD react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "trans:cleanextracted": "if exist extracted rd extracted /S /Q",
    "trans:initextracted": "if not exist extracted md extracted",
    "trans:updateTsConfig": "node src/translations/updateTsConfig.js",
    "trans:compile": "tsc -p . --target ES6 --module esnext --jsx preserve --outDir extracted",
    "trans:extract": "set NODE_ENV=production && babel ./extracted",
    "trans:manage": "node src/translations/manageTranslations.js",
    "trans:translate": "node src/translations/translate.js",
    "trans": "npm run trans:cleanextracted && npm run trans:initextracted && npm run trans:updateTsConfig && npm run trans:compile && npm run trans:extract && npm run trans:manage && npm run trans:translate",
    "preinstall": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions"
  },
  "overrides": {
    "glob-parent": "5.1.2",
    "set-value": "4.0.1",
    "postcss":"^8.4.31"
  },
