{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "columns": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "column_types": {
      "type": "array",
      "items": {
        "type": "string"
        //"enum": [ "int", "str" ] // Assuming only "int" and "str" are allowed column types
      }
    },
    "data": {
      "type": "array",
      "items": {
        "type": "array",
        "items": {
          "oneOf": [
            { "type": "integer" },
            { "type": "string" }
          ]
        }
      }
    },
    "caption": {
      "type": [ "string", "null" ]
    }
  },
  "required": [ "columns", "column_types", "data" ]
}